	blurEffect.BackgroundTransparency = 0.7
	blurEffect.BorderSizePixel = 0
	blurEffect.Visible = false
	blurEffect.ZIndex = 4
	blurEffect.Parent = contentFrame
	
	local configMenu = Instance.new("Frame")
	configMenu.Name = "ConfigMenu"
