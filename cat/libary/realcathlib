			SetClipboard("discord.gg/cathub")
		end
		createTween(joinButton, {BackgroundTransparency = 0.8}, 0.15, Enum.EasingStyle.Quart):Play()
		task.wait(0.15)
		createTween(joinButton, {BackgroundTransparency = 1}, 0.15, Enum.EasingStyle.Quart):Play()
	end)
	
	local floatingButton
