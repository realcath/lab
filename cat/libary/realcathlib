	local colorPadCorner = Instance.new("UICorner")
	colorPadCorner.CornerRadius = UDim.new(0, 6)
	colorPadCorner.Parent = colorPad
	
	local colorPadCursor = Instance.new("Frame")
	colorPadCursor.Size = UDim2.new(0, 8, 0, 8)
	colorPadCursor.Position = UDim2.new(1, -4, 0, -4)
	colorPadCursor.AnchorPoint = Vector2.new(0.5, 0.5)
	colorPadCursor.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	colorPadCursor.BorderSizePixel = 2
