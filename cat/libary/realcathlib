	configPadding.Parent = configScrollFrame
	
	configLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		configScrollFrame.CanvasSize = UDim2.new(0, 0, 0, configLayout.AbsoluteContentSize.Y + 20)
	end)
	
	local closeConfigButton = Instance.new("ImageButton")
	closeConfigButton.Name = "CloseConfigButton"
	closeConfigButton.Size = UDim2.new(0, 28, 0, 28)
	closeConfigButton.Position = UDim2.new(0, 10, 0, 10)
	closeConfigButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	closeConfigButton.BorderSizePixel = 0
	closeConfigButton.Image = "rbxassetid://113187532560127"
	closeConfigButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
	closeConfigButton.ZIndex = 7
	closeConfigButton.Parent = configMenu
	
	local closeConfigCorner = Instance.new("UICorner")
	closeConfigCorner.CornerRadius = UDim.new(0, 8)
	closeConfigCorner.Parent = closeConfigButton
	
	local acrylicToggle = Instance.new("Frame")
	acrylicToggle.Name = "AcrylicToggle"
	acrylicToggle.Size = UDim2.new(1, -20, 0, 35)
	acrylicToggle.BackgroundColor3 = Color3.fromRGB(16, 16, 16)
	acrylicToggle.BorderSizePixel = 0
	acrylicToggle.ZIndex = 6
	acrylicToggle.Parent = configScrollFrame
	
	local acrylicCorner = Instance.new("UICorner")
	acrylicCorner.CornerRadius = UDim.new(0, 8)
	acrylicCorner.Parent = acrylicToggle
	
	local acrylicTitle = Instance.new("TextLabel")
	acrylicTitle.Size = UDim2.new(1, -60, 0, 18)
	acrylicTitle.Position = UDim2.new(0, 12, 0, 8)
	acrylicTitle.BackgroundTransparency = 1
	acrylicTitle.Text = "Acrylic"
	acrylicTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
	acrylicTitle.TextSize = 14
	acrylicTitle.Font = Enum.Font.GothamBold
	acrylicTitle.TextXAlignment = Enum.TextXAlignment.Left
	acrylicTitle.ZIndex = 7
	acrylicTitle.Parent = acrylicToggle
	
	local acrylicBox = Instance.new("TextButton")
	acrylicBox.Size = UDim2.new(0, 40, 0, 20)
	acrylicBox.Position = UDim2.new(1, -50, 0, 8)
	acrylicBox.BackgroundColor3 = catlibConfigs.AcrylicEnabled and Color3.fromRGB(50, 150, 50) or Color3.fromRGB(30, 30, 30)
	acrylicBox.BorderSizePixel = 0
	acrylicBox.Text = ""
	acrylicBox.ZIndex = 7
	acrylicBox.Parent = acrylicToggle
	
	local acrylicBoxCorner = Instance.new("UICorner")
	acrylicBoxCorner.CornerRadius = UDim.new(1, 0)
	acrylicBoxCorner.Parent = acrylicBox
	
	if catlibConfigs.AcrylicEnabled then
		mainFrame.BackgroundTransparency = 0.3
		header.BackgroundTransparency = 0.3
	end
	
	acrylicBox.MouseButton1Click:Connect(function()
		catlibConfigs.AcrylicEnabled = not catlibConfigs.AcrylicEnabled
		
		if catlibConfigs.AcrylicEnabled then
			createTween(acrylicBox, {BackgroundColor3 = Color3.fromRGB(50, 150, 50)}, 0.25, Enum.EasingStyle.Quart):Play()
			createTween(mainFrame, {BackgroundTransparency = 0.3}, 0.3):Play()
			createTween(header, {BackgroundTransparency = 0.3}, 0.3):Play()
		else
			createTween(acrylicBox, {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}, 0.25, Enum.EasingStyle.Quart):Play()
			createTween(mainFrame, {BackgroundTransparency = 0}, 0.3):Play()
			createTween(header, {BackgroundTransparency = 0}, 0.3):Play()
		end
		
		saveConfig(catlibConfigFile, catlibConfigs)
	end)
	
	local colorPickerFrame = Instance.new("Frame")
	colorPickerFrame.Name = "ColorPicker"
