		createTween(imageUnderline, {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}, 0.2, Enum.EasingStyle.Quart):Play()
	end)
	
	imageInput.FocusLost:Connect(function()
		createTween(imageUnderline, {BackgroundColor3 = Color3.fromRGB(60, 60, 60)}, 0.2, Enum.EasingStyle.Quart):Play()
		catlibConfigs.BackgroundImage = imageInput.Text
		backgroundImage.Image = imageInput.Text
		headerBgImage.Image = imageInput.Text
		tabsBgImage.Image = imageInput.Text
		profileBgImage.Image = imageInput.Text
		
		saveConfig(catlibConfigFile, catlibConfigs)
	end)
	
