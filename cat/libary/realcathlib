		floatingIconPadding.PaddingRight = UDim.new(0, 10)
		floatingIconPadding.Parent = floatingButton
		
		local floatingButtonSize = config.FloatingButton.Size or UDim2.new(0, 60, 0, 60)
		
		floatingButton.MouseButton1Click:Connect(function()
			createTween(floatingButton, {Size = UDim2.new(0, floatingButtonSize.X.Offset - 10, 0, floatingButtonSize.Y.Offset - 10)}, 0.1, Enum.EasingStyle.Quart):Play()
			task.wait(0.1)
			createTween(floatingButton, {Size = floatingButtonSize}, 0.15, Enum.EasingStyle.Quart, Enum.EasingDirection.Out):Play()
			
			if mainFrame.Visible then
				mainFrame.Visible = false
				if minimizedBar then
					minimizedBar.Visible = false
				end
			else
				mainFrame.Visible = true
			end
		end)
