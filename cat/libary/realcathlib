	local hueBar = Instance.new("ImageButton")
	hueBar.Size = UDim2.new(0, 20, 0, 180)
	hueBar.Position = UDim2.new(0, 200, 0, 40)
	hueBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	hueBar.BorderSizePixel = 0
	hueBar.Image = "rbxassetid://3641079629"
	hueBar.ZIndex = 7
	hueBar.Parent = colorPickerFrame
	
	local hueBarCorner = Instance.new("UICorner")
	hueBarCorner.CornerRadius = UDim.new(0, 6)
	hueBarCorner.Parent = hueBar
	
	local hueCursor = Instance.new("Frame")
	hueCursor.Size = UDim2.new(1, 4, 0, 4)
	hueCursor.Position = UDim2.new(0, -2, 0, 0)
	hueCursor.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	hueCursor.BorderSizePixel = 2
	hueCursor.BorderColor3 = Color3.fromRGB(0, 0, 0)
	hueCursor.ZIndex = 8
	hueCursor.Parent = hueBar
	
	local hueCursorCorner = Instance.new("UICorner")
	hueCursorCorner.CornerRadius = UDim.new(1, 0)
	hueCursorCorner.Parent = hueCursor
	
	local currentHue = 0
	local currentSat = 1
	local currentVal = 1
	
	local function updateColor()
		local color = Color3.fromHSV(currentHue, currentSat, currentVal)
		catlibConfigs.ThemeColor = color
		
		closeButton.ImageColor3 = color
		maximizeButton.ImageColor3 = color
		minimizeButton.ImageColor3 = color
		configButton.ImageColor3 = color
		
		for _, tab in pairs(tabsFrame:GetChildren()) do
			if tab:IsA("TextButton") then
				local selectionBar = tab:FindFirstChild("SelectionBar")
				if selectionBar then
					selectionBar.BackgroundColor3 = color
				end
				local tabIcon = tab:FindFirstChild("ImageLabel")
				if tabIcon and tabIcon.Name ~= "BackgroundImage" then
					tabIcon.ImageColor3 = color
				end
			end
		end
		
		for _, component in pairs(componentsFrame:GetDescendants()) do
			if component.Name == "DropdownIcon" or component.Name == "ButtonIcon" then
				component.ImageColor3 = color
			end
		end
		
		if resizeHandle then
			resizeHandle.ImageColor3 = color
		end
		
		saveConfig(catlibConfigFile, catlibConfigs)
	end
	
	local colorPadDragging = false
	local hueDragging = false
	
	colorPad.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			colorPadDragging = true
			local pos = (input.Position - colorPad.AbsolutePosition) / colorPad.AbsoluteSize
			currentSat = math.clamp(pos.X, 0, 1)
			currentVal = 1 - math.clamp(pos.Y, 0, 1)
			colorPadCursor.Position = UDim2.new(math.clamp(pos.X, 0, 1), 0, math.clamp(pos.Y, 0, 1), 0)
			updateColor()
		end
	end)
	
	colorPad.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			colorPadDragging = false
		end
	end)
	
	hueBar.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			hueDragging = true
			local pos = math.clamp((input.Position.Y - hueBar.AbsolutePosition.Y) / hueBar.AbsoluteSize.Y, 0, 1)
			currentHue = pos
			hueCursor.Position = UDim2.new(0, -2, pos, 0)
			colorPad.BackgroundColor3 = Color3.fromHSV(currentHue, 1, 1)
			updateColor()
		end
	end)
	
	hueBar.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			hueDragging = false
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if colorPadDragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			local pos = (input.Position - colorPad.AbsolutePosition) / colorPad.AbsoluteSize
			currentSat = math.clamp(pos.X, 0, 1)
			currentVal = 1 - math.clamp(pos.Y, 0, 1)
			colorPadCursor.Position = UDim2.new(math.clamp(pos.X, 0, 1), 0, math.clamp(pos.Y, 0, 1), 0)
			updateColor()
		end
		
		if hueDragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			local pos = math.clamp((input.Position.Y - hueBar.AbsolutePosition.Y) / hueBar.AbsoluteSize.Y, 0, 1)
			currentHue = pos
			hueCursor.Position = UDim2.new(0, -2, pos, 0)
			colorPad.BackgroundColor3 = Color3.fromHSV(currentHue, 1, 1)
			updateColor()
		end
	end)
	
